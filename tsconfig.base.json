{
  "extends": "@tsconfig/node16/tsconfig.json",
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "skipLibCheck": false,
    "useUnknownInCatchVariables": false,
    "baseUrl": ".",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "typeRoots": [
      "local_types",
      "node_modules/@types"
    ]
  }
}
